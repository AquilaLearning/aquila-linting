{
    "cSpell.words": [
        "linebreak"
    ],
    "eslint.enable": true,
    "oxc.enable": true,
    "eslint.workingDirectories": [
        {
            "mode": "auto"
        }
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.oxc": "explicit",
        "source.fixAll.eslint": "explicit"
    }
}
